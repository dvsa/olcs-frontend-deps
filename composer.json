{
  "name": "olcs/olcs-frontend-deps",
  "description": "List of front end dependencies, shared between internal and selfserve nodes",
  "repositories": [
    {
      "packagist": false
    },
    {
      "url": "https://nexus.olcs.dev-dvsacloud.uk/repository/OLCS",
      "type": "composer"
    },
    {
      "url": "https://nexus.olcs.dev-dvsacloud.uk/repository/Packagist-Proxy",
      "type": "composer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-intl": "*",
    "doctrine/annotations": "^1.14.2",
    "doctrine/doctrine-module": "^5",
    "firebase/php-jwt": "^6.0",
    "olcs/olcs-auth": "dev-project/3.0",
    "olcs/olcs-common": "dev-project/3.0",
    "olcs/olcs-logging": "dev-project/3.0",
    "olcs/olcs-transfer": "dev-project/3.0",
    "olcs/olcs-utils": "dev-project/3.0",
    "saeven/zf3-purifier": "^1.1",
    "symfony/filesystem": "^5",
    "symfony/lock": "^5"
  },
  "config": {
    "secure-http": false,
    "sort-packages": true,
    "allow-plugins": {
      "laminas/laminas-component-installer": true,
      "laminas/laminas-dependency-plugin": true,
      "elendev/composer-push": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "nexus-push": {
      "url": "https://nexus.olcs.dev-dvsacloud.uk/repository/olcs-frontend-deps"
    }
  }
}
